<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomInput">
        <attr name="titleInput" format="string" />
        <attr name="hintInput" format="string" />
        <attr name="errorInput" format="string" />
        <attr name="maximumInput" format="integer" />
        <attr name="minimumInput" format="integer" />
        <attr name="typeInput" format="enum">
            <enum name="text" value="1" />
            <enum name="number" value="2" />
            <enum name="alphabet" value="3" />
            <enum name="alphanumeric" value="4" />
            <enum name="datepicker" value="5" />
            <enum name="dropdown" value="6" />
        </attr>
    </declare-styleable>
</resources>